---
import Base from '@layouts/base.astro';

export type Props = {
  title: string,
}

const { title } = Astro.props;
---
<Base>
	<html lang="es">
		<head>
			<meta charset="utf-8" />
			<link rel="icon" type="image/svg+xml" href="/logo.svg" />
			<meta name="viewport" content="width=device-width" />
			<title>{title}</title>
		</head>
			<slot />
		</body>
	</html>
</Base>
